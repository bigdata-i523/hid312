@Article{musichist,
  author   = {Tuppen, Sandra1 and Rose, Stephen2 and Drosopoulou, Loukia},
  title    = {LIBRARY CATALOGUE RECORDS AS A RESEARCH RESOURCE: INTRODUCING 'A BIG DATA HISTORY OF MUSIC'.},
