  keywords = {Records -- Access control, Records management, Big data, Library science, Humanities},
  url      = {http://proxyiub.uits.iu.edu/login?url=https://search.ebscohost.com/login.aspx?direct=true&db=llf&AN=114128249&site=eds-live&scope=site},
